smbclient --list "$TARGET"
smbclient --no-pass --user '' --list "$TARGET"
smbclient --no-pass --user '' "//$TARGET/SYSVOL"
smbclient --user "$USER" --password "$PASSWORD" --list "$TARGET"
smbclient --user "$USER" --password "$PASSWORD" "//$TARGET/SYSVOL"
smbclient --user "$USER" --password "$NT_HASH" --pw-nt-hash --list "$TARGET"
smbclient --user "$USER" --password "$NT_HASH" --pw-nt-hash "//$TARGET/SYSVOL"
rpcclient -U "$DOMAIN"/"$USER" "$TARGET"
rpcclient -U '' -N "$DC_HOST"
rpcclient -U '' -N "$DC_HOST" -c "querydominfo;netshareenumall"